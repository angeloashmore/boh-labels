{
  "name": "boh-labels",
  "productName": "BOH Labels",
  "version": "0.1.2",
  "private": true,
  "main": "main.js",
  "scripts": {
    "lint": "standard",
    "start": "ENVIRONMENT=DEV electron .",
    "build": "webpack",
    "watch": "webpack-dev-server --hot --inline",
    "dev": "concurrently --kill-others \"npm run watch\" \"npm run start\"",
    "package": "npm run build && electron-packager . --ignore=node_modules --platform=darwin --arch=x64 --overwrite --out=release --icon=src/assets/icon.icns"
  },
  "devDependencies": {
    "babel-core": "^6.8.0",
    "babel-eslint": "^6.1.0",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "concurrently": "^2.1.0",
    "css-loader": "^0.23.1",
    "electron-packager": "^7.0.1",
    "electron-prebuilt": "^1.1.0",
    "json-loader": "^0.5.4",
    "postcss-inline-svg": "^1.4.0",
    "postcss-loader": "^0.9.1",
    "postcss-nested": "^1.0.0",
    "standard": "^7.1.2",
    "style-loader": "^0.13.1",
    "svg-url-loader": "^1.0.3",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "app-reset": "^1.0.2",
    "classnames": "^2.2.5",
    "i": "^0.3.5",
    "immutable": "^3.8.1",
    "months": "^1.0.1",
    "react": "^15.2.0",
    "react-barcode": "^1.0.5",
    "react-css-modules": "^3.7.6",
    "react-dom": "^15.0.2",
    "react-infinite": "^0.9.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.0",
    "react-router-redux": "^4.0.4",
    "redux": "^3.5.2",
    "redux-actions": "^0.9.1",
    "redux-logger": "^2.6.1",
    "redux-promise-middleware": "^3.0.0",
    "redux-thunk": "^2.1.0",
    "temp": "^0.8.3",
    "type-to-reducer": "^1.0.1",
    "whatwg-fetch": "^1.0.0"
  },
  "standard": {
    "globals": [
      "fetch"
    ],
    "ignore": [
      "/dist/",
      "/release/"
    ],
    "parser": "babel-eslint"
  }
}
